<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
  
  <configuration>
  	<!-- 아래의 "typeAliases"태그는 *-Mapper.xml파일에서 쿼리의 결과로 반환되는 값을 받는데 사용될 타입을 선언하는 곳이다. -->
  	<typeAliases>
  		<typeAlias type="java.lang.Integer" alias="int"/>
  		<typeAlias type="java.lang.String" alias="str" />
  		<typeAlias type="com.spring.dto.MemberVO" alias="member"/>
  		<typeAlias type="com.spring.dto.FollowWriteVO" alias="followBoard"/>
  		<typeAlias type="com.spring.dto.VoteVO" alias="vote"/>
  		<typeAlias type="com.spring.dto.ReviewVO" alias="review"/>
  		<typeAlias type="com.spring.dto.ReviewAndTogetherVO" alias="reviewTogether"/>
  		<typeAlias type="com.spring.dto.AttachVO" alias="attach"/>
  		<typeAlias type="com.spring.dto.CommentsVO" alias="comments"/>
  		<typeAlias type="com.spring.dto.TogetherVO" alias="together"/>
  		<typeAlias type="com.spring.dto.LikeVO" alias="like"/>
  		<typeAlias type="com.spring.dto.MessageVO" alias="message"/>
  		<typeAlias type="com.spring.dto.ReportVO" alias="report"/>
  		<typeAlias type="com.spring.dto.Report_CVO" alias="reportC"/>
  		<typeAlias type="com.spring.dto.CommentsBoardVO" alias="commentsBoard"/>
  		<typeAlias type="com.spring.dto.HoldingVO" alias="holding"/>
  		<typeAlias type="com.spring.dto.FestivalVO" alias="festival"/>
  	</typeAliases>
  	
  </configuration>
